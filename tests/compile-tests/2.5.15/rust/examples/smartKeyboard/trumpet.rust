	pub fn instanceClear(&mut self) {
		let mut l0: i32 = 0;
		loop {
			self.fRec2[l0 as usize] = 0.0;
			l0 = (l0 + 1);
			if (l0 < 2) { continue; } else { break; }
		}
		let mut l1: i32 = 0;
		loop {
			self.fVec0[l1 as usize] = 0.0;
