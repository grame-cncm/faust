		let mut l7: i32 = 0;
		loop {
			self.fRec0[l7 as usize] = 0.0;
			l7 = (l7 + 1);
			if (l7 < 3) { continue; } else { break; }
		}
		
	}
	
	pub fn instanceConstants(&mut self, samplingFreq: i32) {
		self.fSamplingFreq = samplingFreq;
		self.fConst0 = f32::min(192000.0, f32::max(1.0, (self.fSamplingFreq as f32)));
		self.fConst1 = (3.14159274 / self.fConst0);
		self.fConst2 = (1.0 / self.fConst0);
		
	}
	
	pub fn instanceInit(&mut self, samplingFreq: i32) {
		self.instanceConstants(samplingFreq);
		self.instanceResetUserInterface();
		self.instanceClear();
	}
	
	pub fn init(&mut self, samplingFreq: i32) {
		mydsp::classInit(samplingFreq);
		self.instanceInit(samplingFreq);
	}
	
	pub fn buildUserInterface(&mut self, ui_interface: &mut UI<f32>) {
		ui_interface.openVerticalBox("trumpet");
		ui_interface.declare(&mut self.fHslider4, "midi", "pitchwheel");
		ui_interface.addHorizontalSlider("bend", &mut self.fHslider4, 1.0, 0.0, 10.0, 0.01);
		ui_interface.addHorizontalSlider("freq", &mut self.fHslider3, 300.0, 50.0, 2000.0, 0.01);
		ui_interface.addHorizontalSlider("gain", &mut self.fHslider2, 1.0, 0.0, 1.0, 0.01);
		ui_interface.addButton("gate", &mut self.fButton0);
		ui_interface.declare(&mut self.fHslider1, "midi", "ctrl 64");
		ui_interface.addHorizontalSlider("sustain", &mut self.fHslider1, 0.0, 0.0, 1.0, 1.0);
		ui_interface.declare(&mut self.fHslider0, "midi", "ctrl 1");
		ui_interface.addHorizontalSlider("y", &mut self.fHslider0, 1.0, 0.0, 1.0, 0.001);
		ui_interface.closeBox();
		
	}
	
	pub fn compute(&mut self, count: i32, inputs: &[&[f32]], outputs: &mut[&mut[f32]]) {
		let mut fSlow0: f32 = (0.00100000005 * (self.fHslider0 as f32));
		let mut fSlow1: f32 = f32::min(1.0, ((self.fButton0 as f32) + (self.fHslider1 as f32)));
		let mut fSlow2: f32 = (0.00100000005 * (fSlow1 * (self.fHslider2 as f32)));
		let mut fSlow3: f32 = (self.fHslider3 as f32);
		let mut fSlow4: f32 = (self.fHslider4 as f32);
		let mut iSlow5: i32 = ((fSlow1 == 0.0) as i32);
		let mut i: i32 = 0;
		loop {
			self.fRec2[0] = (fSlow0 + (0.999000013 * self.fRec2[1]));
			let mut fTemp0: f32 = f32::tan((self.fConst1 * ((4000.0 * self.fRec2[0]) + 50.0)));
			let mut fTemp1: f32 = (1.0 / fTemp0);
			let mut fTemp2: f32 = (fTemp1 + 1.0);
			self.fVec0[0] = fSlow1;
			self.fRec3[0] = (fSlow2 + (0.999000013 * self.fRec3[1]));
			let mut iTemp3: i32 = (((fSlow1 == self.fVec0[1]) as i32) | iSlow5);
			self.fRec6[0] = ((fSlow4 * (1.0 - (0.999000013 * (iTemp3 as f32)))) + (0.999000013 * ((iTemp3 as f32) * self.fRec6[1])));
			let mut fTemp4: f32 = f32::max(1.00000001e-07, f32::abs((fSlow3 * self.fRec6[0])));
			let mut fTemp5: f32 = (self.fRec4[1] + (self.fConst2 * fTemp4));
			let mut fTemp6: f32 = (fTemp5 + -1.0);
			let mut iTemp7: i32 = ((fTemp6 < 0.0) as i32);
			self.fRec4[0] = if (iTemp7 as i32 == 1) { fTemp5 } else { fTemp6 };
			let mut fRec5: f32 = if (iTemp7 as i32 == 1) { fTemp5 } else { (fTemp5 + ((1.0 - (self.fConst0 / fTemp4)) * fTemp6)) };
			let mut fTemp8: f32 = (self.fRec3[0] * ((2.0 * fRec5) + -1.0));
			self.fVec1[0] = fTemp8;
			self.fRec1[0] = ((self.fRec1[1] * (0.0 - ((1.0 - fTemp1) / fTemp2))) + ((fTemp8 + self.fVec1[1]) / fTemp2));
			let mut fTemp9: f32 = (((fTemp1 + 1.0) / fTemp0) + 1.0);
			self.fRec0[0] = (self.fRec1[0] - (((self.fRec0[2] * (((fTemp1 + -1.0) / fTemp0) + 1.0)) + (2.0 * (self.fRec0[1] * (1.0 - (1.0 / mydsp_faustpower2_f(fTemp0)))))) / fTemp9));
			let mut fTemp10: f32 = ((self.fRec0[2] + (self.fRec0[0] + (2.0 * self.fRec0[1]))) / fTemp9);
			outputs[0][i as usize] = (fTemp10 as f32);
			outputs[1][i as usize] = (fTemp10 as f32);
			self.fRec2[1] = self.fRec2[0];
			self.fVec0[1] = self.fVec0[0];
			self.fRec3[1] = self.fRec3[0];
			self.fRec6[1] = self.fRec6[0];
			self.fRec4[1] = self.fRec4[0];
			self.fVec1[1] = self.fVec1[0];
			self.fRec1[1] = self.fRec1[0];
			self.fRec0[2] = self.fRec0[1];
			self.fRec0[1] = self.fRec0[0];
			i = (i + 1);
			if (i < count) { continue; } else { break; }
		}
		
	}

