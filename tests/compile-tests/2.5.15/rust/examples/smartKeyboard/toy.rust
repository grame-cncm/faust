		let mut fSlow4: f32 = (0.00100000005 * (self.fHslider4 as f32));
		let mut fSlow5: f32 = (0.00100000005 * (self.fHslider5 as f32));
		let mut i: i32 = 0;
		loop {
			self.fRec1[0] = (fSlow0 + (0.999000013 * self.fRec1[1]));
			self.fRec2[0] = (fSlow1 + (0.999000013 * self.fRec2[1]));
			self.fRec5[0] = (fSlow2 + (0.999000013 * self.fRec5[1]));
			self.fVec0[0] = ((20.0 * self.fRec5[0]) + 2.0);
			let mut fTemp0: f32 = (self.fRec4[1] + (self.fConst1 * self.fVec0[1]));
			self.fRec4[0] = (fTemp0 - f32::floor(fTemp0));
			self.fRec6[0] = (fSlow3 + (0.999000013 * self.fRec6[1]));
			let mut fTemp1: f32 = f32::tan((self.fConst2 * ((3000.0 * self.fRec6[0]) + 300.0)));
			let mut fTemp2: f32 = (1.0 / fTemp1);
			self.fRec7[0] = (fSlow4 + (0.999000013 * self.fRec7[1]));
			let mut fTemp3: f32 = (1.0 / self.fRec7[0]);
			let mut fTemp4: f32 = (((fTemp2 + fTemp3) / fTemp1) + 1.0);
			self.fRec3[0] = (((((self.fRec4[0] - self.fRec4[1]) < 0.0) as i32) as f32) - (((self.fRec3[2] * (((fTemp2 - fTemp3) / fTemp1) + 1.0)) + (2.0 * (self.fRec3[1] * (1.0 - (1.0 / mydsp_faustpower2_f(fTemp1)))))) / fTemp4));
			self.fRec0[(self.IOTA & 131071) as usize] = ((self.fRec1[0] * self.fRec0[((self.IOTA - ((f32::min(65536.0, f32::max(0.0, (self.fConst0 * self.fRec2[0]))) as i32) + 1)) & 131071) as usize]) + ((self.fRec3[2] + (self.fRec3[0] + (2.0 * self.fRec3[1]))) / fTemp4));
			self.fRec8[0] = (fSlow5 + (0.999000013 * self.fRec8[1]));
			let mut fTemp5: f32 = f32::max(-1.0, f32::min(1.0, (self.fRec0[((self.IOTA - 0) & 131071) as usize] * f32::powf(10.0, (2.0 * self.fRec8[0])))));
			let mut fTemp6: f32 = (0.949999988 * (fTemp5 * (1.0 - (0.333333343 * mydsp_faustpower2_f(fTemp5)))));
			outputs[0][i as usize] = (fTemp6 as f32);
			outputs[1][i as usize] = (fTemp6 as f32);
			self.fRec1[1] = self.fRec1[0];
			self.fRec2[1] = self.fRec2[0];
			self.fRec5[1] = self.fRec5[0];
			self.fVec0[1] = self.fVec0[0];
			self.fRec4[1] = self.fRec4[0];
			self.fRec6[1] = self.fRec6[0];
			self.fRec7[1] = self.fRec7[0];
			self.fRec3[2] = self.fRec3[1];
			self.fRec3[1] = self.fRec3[0];
			self.IOTA = (self.IOTA + 1);
			self.fRec8[1] = self.fRec8[0];
