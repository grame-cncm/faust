
<faust>
	<name>freeverb</name>
	<author>Grame</author>
	<copyright>(c)GRAME 2006</copyright>
	<license>BSD</license>
	<version>1.0</version>
	<classname>mydsp</classname>
	<inputs>2</inputs>
	<outputs>2</outputs>
	<ui>
		<activewidgets>
			<count>3</count>
			<widget type="hslider" id="1">
				<label>Damp</label>
				<varname>fslider1</varname>
				<init>0.5f</init>
				<min>0.0f</min>
				<max>1.0f</max>
				<step>0.025f</step>
			</widget>
			<widget type="hslider" id="2">
				<label>RoomSize</label>
				<varname>fslider2</varname>
				<init>0.5f</init>
				<min>0.0f</min>
				<max>1.0f</max>
				<step>0.025f</step>
			</widget>
			<widget type="hslider" id="3">
				<label>Wet</label>
				<varname>fslider0</varname>
				<init>0.3333f</init>
				<min>0.0f</min>
				<max>1.0f</max>
				<step>0.025f</step>
			</widget>
		</activewidgets>
		
		<passivewidgets>
			<count>0</count>
		</passivewidgets>
		
		<layout>
			<group type="vgroup">
				<label>Freeverb</label>
				<widgetref id="1" />
				<widgetref id="2" />
				<widgetref id="3" />
			</group>
		</layout>
	</ui>
</faust>
