
<faust>
	<name>pitch-shifter</name>
	<author>Grame</author>
	<copyright>(c)GRAME 2006</copyright>
	<license>BSD</license>
	<version>1.0</version>
	<classname>mydsp</classname>
	<inputs>1</inputs>
	<outputs>1</outputs>
	<ui>
		<activewidgets>
			<count>3</count>
			<widget type="hslider" id="1">
				<label>shift (semitones)</label>
				<varname>fslider0</varname>
				<init>0.0f</init>
				<min>-12.0f</min>
				<max>12.0f</max>
				<step>0.1f</step>
			</widget>
			<widget type="hslider" id="2">
				<label>window (samples)</label>
				<varname>fslider1</varname>
				<init>1e+03f</init>
				<min>5e+01f</min>
				<max>1e+04f</max>
				<step>1.0f</step>
			</widget>
			<widget type="hslider" id="3">
				<label>xfade (samples)</label>
				<varname>fslider2</varname>
				<init>1e+01f</init>
				<min>1.0f</min>
				<max>1e+04f</max>
				<step>1.0f</step>
			</widget>
		</activewidgets>
		
		<passivewidgets>
			<count>0</count>
		</passivewidgets>
		
		<layout>
			<group type="vgroup">
				<label>Pitch Shifter</label>
				<widgetref id="1" />
				<widgetref id="2" />
				<widgetref id="3" />
			</group>
		</layout>
	</ui>
</faust>
